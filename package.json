  "scripts": {
    "precommit": "run-s lint test",
    "start": "node src/client/server.js",
    "start:dev": "ts-node src/server/index.ts",
    "start:prod": "node dist/server/server/index.js",
    "build:server": "tsc -p tsconfig.server.json",
    "test": "cross-env NODE_ENV=test run-s test:*",
    "test:admin": "cross-env NODE_ENV=test run-s test:admin:*",
    "test:admin:app": "cross-env NODE_ENV=test mocha \"./src/admin/**/!(server).test.ts*\" --compilers ts:./ts-node-register,tsx:./ts-node-register --require jsdom.setup.js",
    "test:admin:server": "cross-env NODE_ENV=test mocha src/admin/server.test.tsx --compilers ts:./ts-node-register,tsx:./ts-node-register --require jsdom.setup.js",
    "test:api": "cross-env NODE_ENV=test mocha src/server/**/*.test.ts --compilers ts:./ts-node-register",
    "coverage:server": "nyc --reporter=html --reporter=lcov --reporter=text npm run test:server",
    "lint": "tslint src/server/**/*.ts",
    "clean": "rimraf ./src/**/*.js.map && rimraf ./src/**/*.js"
  },
